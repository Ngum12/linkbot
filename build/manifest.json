{
  "manifest_version": 2,
  "name": "Link Library Chatbot",
  "version": "1.0",
  "description": "A chatbot for Link Library",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["static/js/main.abf361c4.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon48.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}